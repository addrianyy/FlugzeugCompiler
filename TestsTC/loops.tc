extern i32 blackbox(i32 x);

i32 test(i32 n) {
    i32 result = 0;

    for (i32 i = 0; i < n; i += 1) {
        result += blackbox(i);
    }

    return result;
}

i32 testx(i32 n) {
    i32 result = 0;
    i32 i = 0;

    while (i < n) {
        result += blackbox(i);

        result += 2;

        if (result == 99) {
            continue;
        }

        result += 3;
        i += 1;
    }

    return result;
}